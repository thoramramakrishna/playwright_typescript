{
  "name": "ui-hw-benplanlib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --host=localhost",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --configuration test",
    "check-format": "prettier -c src",
    "format": "prettier -w src",
    "build:jenkinsK8s": "ng build --configuration production --base-href /ui-hw-benplanlib/",
    "build:jenkinsArtifactoryAndWar": "cd dist/ui-hw-benplanlib && jar -cf ui-hw-benplanlib.war * && cd ../../ && mkdir build && mv dist/ui-hw-benplanlib/ui-hw-benplanlib.war build/",
    "run:jenkinsRunNonE2EUnitTests": "npx ng test --progress=false --code-coverage --watch=false --configuration test",
    "playwright": "npx playwright",
    "playwright.stage": "cross-env RUN_TIME=stage npx playwright test ./Integration-E2E/regionalplanmapping/regional-planmapping-stage.spec.ts --grep @stage",
    "playwright.ci.test": "cross-env RUN_TIME=ci npx playwright test ./Integration-E2E --grep-invert @stage",
    "playwright.ci": "npm run clean-istanbul && npm run setup-istanbul && npm run playwright.ci.test",
    "nyc.report":"npx nyc report",
    "clean-istanbul": "rimraf ./.nyc_output ./coverage ./tests/babel-istanbul/test-results",
    "setup-istanbul": "npx cpy ./angular.json ./temp  --flat && npx cpy ./angular-configs/angular-babel-istanbul.json ./ --flat --rename=angular.json",
    "post-istanbul": "npx cpy ./temp/angular.json ./ --flat --rename=angular.json",
    "open-istanbul-coverage": "open-cli ./coverage/index.html"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "28.2.1",
    "@ag-grid-community/client-side-row-model": "28.2.1",
    "@ag-grid-community/core": "28.2.1",
    "@angular/animations": "14.2.7",
    "@angular/cdk": "14.2.5",
    "@angular/common": "14.2.7",
    "@angular/compiler": "14.2.7",
    "@angular/core": "14.2.7",
    "@angular/forms": "14.2.7",
    "@angular/material": "14.2.5",
    "@angular/platform-browser": "14.2.7",
    "@angular/platform-browser-dynamic": "14.2.7",
    "@angular/router": "14.2.7",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@thebespokepixel/es-tinycolor": "3.1.0",
    "@trinet/angular-design-system": "20230603.1.6",
    "@trinet/ngx-translate-cms-loader": "14.0.0",
    "@types/tinycolor2": "1.4.3",
    "ag-grid-angular": "28.2.1",
    "ag-grid-community": "28.2.1",
    "ag-grid-enterprise": "28.2.1",
    "cross-env": "^7.0.3",
    "dset": "^3.1.2",
    "lodash": "^4.17.21",
    "material-community-components": "13.0.0",
    "moment": "^2.29.4",
    "ngx-ui-tour-core": "6.0.0",
    "ngx-ui-tour-md-menu": "6.0.0",
    "rxjs": "7.4.0",
    "sudo": "^1.0.3",
    "tinycolor2": "1.4.2",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.6",
    "@angular/cli": "14.2.6",
    "@angular/compiler-cli": "14.2.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.38.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.4.9",
    "@types/object-hash": "^3.0.3",
    "babel-plugin-istanbul": "^6.1.1",
    "cpy-cli": "^4.2.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-build-plus": "^14.0.0",
    "nyc": "^15.1.0",
    "object-hash": "^3.0.0",
    "open-cli": "^7.2.0",
    "playwright-test-coverage": "^1.2.12",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "**/coverage/**",
      "playwright.config.ts",
      "**/*.spec.ts",
      "**/*.spec.ts",
      "src/test.ts",
      "src/test-utils/**/*",
      ",**/polyfills.ts",
      "karma.conf.js",
      "**/*/object.ts",
      "**/*.module.ts",
      "**/*.routes.ts",
      "../*.d.ts",
      "**/browser.ts",
      "**/shims_for_IE.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  }
}
